{
  "name": "@xache/sdk",
  "version": "5.5.0",
  "description": "Official TypeScript SDK for Xache Protocol - decentralized agent memory and collective intelligence",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "xache",
    "ai",
    "agent",
    "memory",
    "blockchain",
    "decentralized",
    "collective-intelligence"
  ],
  "author": "Xache Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xacheai/xache-protocol"
  },
  "homepage": "https://xache.xyz",
  "dependencies": {
    "@coinbase/x402": "^0.7.0",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@scure/bip32": "^1.3.3",
    "@solana/spl-token": "^0.3.11",
    "@solana/web3.js": "^1.87.0",
    "bip39": "^3.1.0",
    "bs58": "^5.0.0",
    "ethers": "^6.9.0",
    "libsodium-wrappers": "^0.7.11",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
